<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="mac.yk.devicemanagement">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".controller.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".controller.activity.DetailActivity" />
        <activity android:name=".controller.activity.SaveActivity" />
        <activity android:name=".controller.activity.Splash" />
        <activity android:name=".controller.activity.LoginActivity" />
        <activity android:name=".controller.activity.RecordActivity" />
        <activity android:name=".controller.activity.SetActivity" />
        <activity android:name=".controller.activity.GestureActivity"></activity>
    </application>

</manifest>